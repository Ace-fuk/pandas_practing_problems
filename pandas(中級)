        "import pandas as pd\n",
        "\n",
        "# ãƒ‡ãƒ¼ã‚¿æº–å‚™\n",
        "employees = pd.DataFrame({\n",
        "    \"ç¤¾å“¡ID\": [101, 102, 103, 104, 105, 106],\n",
        "    \"åå‰\": [\"ç”°ä¸­\", \"ä½è—¤\", \"éˆ´æœ¨\", \"é«˜æ©‹\", \"ä¼Šè—¤\", \"ä¸­æ‘\"],\n",
        "    \"éƒ¨ç½²\": [\"å–¶æ¥­\", \"ç·å‹™\", \"å–¶æ¥­\", \"é–‹ç™º\", \"ç·å‹™\", \"é–‹ç™º\"],\n",
        "    \"å¹´é½¢\": [28, 35, 30, 26, 40, 29],\n",
        "    \"çµ¦ä¸\": [500, 600, 550, 480, 620, 530]\n",
        "})\n",
        "\n",
        "projects = pd.DataFrame({\n",
        "    \"ç¤¾å“¡ID\": [101, 103, 104, 105, 106],\n",
        "    \"ãƒ—ãƒ­ã‚¸ã‚§ã‚¯ãƒˆ\": [\"A\", \"B\", \"A\", \"C\", \"B\"],\n",
        "    \"ç¨¼åƒæ™‚é–“\": [120, 150, 100, 200, 180]\n",
        "})\n",
        "\n",
        "# 1. è¤‡æ•°æ¡ä»¶æŠ½å‡º\n",
        "å–¶æ¥­_çµ¦ä¸500ä»¥ä¸Š = employees[(employees[\"éƒ¨ç½²\"]==\"å–¶æ¥­\") & (employees[\"çµ¦ä¸\"]>=500)]\n",
        "print(\"éƒ¨ç½²ãŒå–¶æ¥­ã‹ã¤çµ¦ä¸500ä»¥ä¸Š:\\n\", å–¶æ¥­_çµ¦ä¸500ä»¥ä¸Š, \"\\n\")\n",
        "\n",
        "å¹´é½¢30ä»¥ä¸Š_çµ¦ä¸600ä»¥ä¸Š = employees[(employees[\"å¹´é½¢\"]>=30) & (employees[\"çµ¦ä¸\"]>=600)]\n",
        "print(\"å¹´é½¢30ä»¥ä¸Šã‹ã¤çµ¦ä¸600ä»¥ä¸Š:\\n\", å¹´é½¢30ä»¥ä¸Š_çµ¦ä¸600ä»¥ä¸Š, \"\\n\")\n",
        "\n",
        "# 2. è¤‡æ•°åˆ—ã§ã®ä¸¦ã¹æ›¿ãˆ\n",
        "ä¸¦ã¹æ›¿ãˆ = employees.sort_values([\"éƒ¨ç½²\",\"çµ¦ä¸\",\"å¹´é½¢\"], ascending=[True, False, True])\n",
        "print(\"éƒ¨ç½²â†’çµ¦ä¸é™é †â†’å¹´é½¢æ˜‡é †:\\n\", ä¸¦ã¹æ›¿ãˆ, \"\\n\")\n",
        "\n",
        "# 3. groupby ã¨é›†è¨ˆ\n",
        "éƒ¨ç½²ã”ã¨ã®å¹³å‡ = employees.groupby(\"éƒ¨ç½²\")[[\"çµ¦ä¸\",\"å¹´é½¢\"]].mean()\n",
        "print(\"éƒ¨ç½²ã”ã¨ã®å¹³å‡çµ¦ä¸ãƒ»å¹³å‡å¹´é½¢:\\n\", éƒ¨ç½²ã”ã¨ã®å¹³å‡, \"\\n\")\n",
        "\n",
        "ãƒ—ãƒ­ã‚¸ã‚§ã‚¯ãƒˆã”ã¨ã®å¹³å‡ç¨¼åƒ = projects.groupby(\"ãƒ—ãƒ­ã‚¸ã‚§ã‚¯ãƒˆ\")[\"ç¨¼åƒæ™‚é–“\"].mean()\n",
        "print(\"ãƒ—ãƒ­ã‚¸ã‚§ã‚¯ãƒˆã”ã¨ã®å¹³å‡ç¨¼åƒæ™‚é–“:\\n\", ãƒ—ãƒ­ã‚¸ã‚§ã‚¯ãƒˆã”ã¨ã®å¹³å‡ç¨¼åƒ, \"\\n\")\n",
        "\n",
        "# 4. apply + lambda å¿œç”¨\n",
        "employees[\"å¹´é½¢ã‚°ãƒ«ãƒ¼ãƒ—\"] = employees[\"å¹´é½¢\"].apply(\n",
        "    lambda x: \"è‹¥æ‰‹\" if x<30 else (\"ãƒ™ãƒ†ãƒ©ãƒ³\" if x>=40 else \"ä¸­å …\")\n",
        ")\n",
        "projects[\"ç¨¼åƒãƒ¬ãƒ™ãƒ«\"] = projects[\"ç¨¼åƒæ™‚é–“\"].apply(lambda x: \"å¤šã„\" if x>=150 else \"å°‘ãªã„\")\n",
        "print(\"å¹´é½¢ã‚°ãƒ«ãƒ¼ãƒ—:\\n\", employees[[\"åå‰\",\"å¹´é½¢\",\"å¹´é½¢ã‚°ãƒ«ãƒ¼ãƒ—\"]], \"\\n\")\n",
        "print(\"ç¨¼åƒãƒ¬ãƒ™ãƒ«:\\n\", projects, \"\\n\")\n",
        "\n",
        "# 5. ãƒ‡ãƒ¼ã‚¿ãƒ•ãƒ¬ãƒ¼ãƒ ã®çµåˆ\n",
        "df_merged = pd.merge(employees, projects, on=\"ç¤¾å“¡ID\", how=\"left\")\n",
        "print(\"çµåˆå¾Œ:\\n\", df_merged, \"\\n\")\n",
        "\n",
        "çµ¦ä¸500ä»¥ä¸Š_ç¨¼åƒå¤šã„ = df_merged[(df_merged[\"çµ¦ä¸\"]>=500) & (df_merged[\"ç¨¼åƒãƒ¬ãƒ™ãƒ«\"]==\"å¤šã„\")]\n",
        "print(\"çµ¦ä¸500ä»¥ä¸Šã‹ã¤ç¨¼åƒå¤šã„:\\n\", çµ¦ä¸500ä»¥ä¸Š_ç¨¼åƒå¤šã„)\n"
